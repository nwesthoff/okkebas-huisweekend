  "author": "Nils Westhoff <nils@nilswesthoff.com>",
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "acorn": "^6.1.1",
    "core-js": "^2.6.5",
    "gatsby": "^2.3.6",
    "gatsby-cli": "^2.5.4",
    "gatsby-image": "^2.0.37",
    "gatsby-mdx": "^0.4.4",
    "gatsby-plugin-manifest": "^2.0.26",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.0.12",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-embed-video": "^1.7.1",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-static-images": "^1.1.0",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "mdx-utils": "0.0.2",
    "node-sass": "^4.11.0",
    "prism-react-renderer": "^0.1.6",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-testing-library": "^6.0.4",
    "sharp": "^0.22.0",
    "styled-components": "^4.2.0"
  },
  "keywords": [
    "gatsby",
    "mdx",
    "typescript",
    "prismjs",
    "styled-components"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "npm run develop",
    "test": "jest"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.20.3",
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "@types/jest": "^24.0.11",
    "@types/md5": "^2.1.33",
    "@types/react": "^16.8.10",
    "@types/react-helmet": "^5.0.8",
    "@types/styled-components": "^4.1.12",
    "babel-plugin-styled-components": "^1.10.0",
    "jest": "^24.6.0",
    "jest-dom": "^3.1.3",
    "loader-utils": "^1.2.3",
    "prettier": "^1.16.4",
    "sass": "^1.17.3",
    "ts-jest": "^24.0.1",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tylergreulich/gatsby-typescript-mdx-prismjs-starter"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  }
}
